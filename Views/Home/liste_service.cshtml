@model Service[]
      <main>
        <div class="head-title">
          <div class="left">
            <h1>Services</h1>
            <ul class="breadcrumb">
              <li>
                <a href="#">Liste</a>
              </li>
              <li><i class="bx bx-chevron-right"></i></li>
              <li>
                <a class="active" href="#">Home</a>
              </li>
            </ul>
          </div>
        </div>

        <ul class="box-info" id="liste_carre_container">
          @foreach (var service in Model)
        {
          <li>
            <a href="">
              <div class="liste_carre">
                <img src="~/images/@service.IconeService" alt="" />
                <span class="text liste_carre_text liste_carre_text">
                  <h4>@service.NomService</h4>
                </span>
              </div>
            </a>
          </li>
            
    
        }
        </ul>
      </main>